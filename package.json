{
  "name": "insoftex-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "tsc -b tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node --gc_interval=100 dist/src/http-server.js",
    "start:dev": "node --gc_interval=100 -r dotenv-safe/config dist/src/http-server.js",
    "start:ts": "ts-node -T -r dotenv-safe/config src/http-server.ts",
    "start:js:watch": "nodemon --exec 'npm run start:dev'",
    "start:ts:watch": "nodemon --exec 'npm run start:ts'",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migration:create:dev": "npm run typeorm:cli:dev -- migration:create ./src/migration/$npm_config_name",
    "migration:generate:dev": "npm run typeorm:cli:dev -- -d ./src/ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:revert:dev": "npm run typeorm:cli:dev -- -d ./src/ormconfig.ts migration:revert",
    "migration:revert": "npm run typeorm:cli -- -d dist/src/ormconfig.js migration:revert",
    "migration:run:dev": "npm run typeorm:cli:dev migration:run -- -d ./src/ormconfig.ts",
    "migration:run": "npm run typeorm:cli migration:run -- -d ./dist/src/ormconfig.js",
    "typeorm:cli:dev": "ts-node -T -r dotenv-safe/config ./node_modules/typeorm/cli",
    "typeorm:cli": "node --gc_interval=100 ./node_modules/typeorm/cli.js",
    "worker:seed-vaccines:dev": "ts-node -T -r dotenv-safe/config src/worker/SeedVaccineWorker.ts",
    "worker:seed-vaccines": "node --gc_interval=100 dist/src/worker/SeedVaccineWorker.js"
  },
  "dependencies": {
    "@nestjs/common": "9.0.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/throttler": "3.1.0",
    "@nestjs/typeorm": "9.0.1",
    "axios": "0.24.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "csv-parse": "5.3.1",
    "luxon": "3.1.0",
    "mongodb": "3.7.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "typeorm": "0.3.10",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.8",
    "@types/luxon": "3.0.2",
    "@types/node": "16.0.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "dotenv-safe": "8.2.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.3",
    "mongodb-memory-server": "8.9.5",
    "nodemon": "2.0.20",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "9.2.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
  }
}
